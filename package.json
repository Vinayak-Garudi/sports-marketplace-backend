{
  "name": "node-express-mongodb-template",
  "version": "1.0.0",
  "description": "A Node.js backend template with Express.js, MongoDB, Mongoose, and CLI generator",
  "main": "src/server.js",
  "scripts": {
    "start": "NODE_ENV=production node src/server.js",
    "dev": "NODE_ENV=local nodemon src/server.js",
    "dev:development": "NODE_ENV=development nodemon src/server.js",
    "start:development": "NODE_ENV=development node src/server.js",
    "start:production": "NODE_ENV=production node src/server.js",
    "test": "jest",
    "generate": "node cli/generator.js",
    "lint": "eslint src/ --fix",
    "format": "prettier --write src/"
  },
  "keywords": [
    "nodejs",
    "express",
    "mongodb",
    "mongoose",
    "backend",
    "template",
    "cli-generator"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "express-rate-limit": "^6.10.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "joi": "^17.9.2",
    "morgan": "^1.10.0",
    "compression": "^1.7.4"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.6.2",
    "supertest": "^6.3.3",
    "eslint": "^8.47.0",
    "prettier": "^3.0.2",
    "commander": "^11.0.0",
    "inquirer": "^9.2.10"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}